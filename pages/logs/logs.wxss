    color: #6f6f6f;
  display: block;
}

.divc .divc_v .divc_b a .iconfont,.divc .divc_v .divd_b a .iconfont{
	font-size: 32rpx;
	color: #9a6912;
    line-height: 80rpx;
}

.divc .divc_v .divd_b a .iconfont{
	color: #2985a0;
}

